@model SDSDSoftwareApplication.ViewModel.EditApplicationRole
@{
    ViewData["Title"] = "DeleteRole";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<h3>Are you sure you want to delete this role " @Html.DisplayFor(model => model.RoleName) " ?</h3>
<div>
  
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.RoleName)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.RoleName)
        </dd>
    </dl>

    <form asp-action="DeleteRole" asp-controller="Admin">
        <input type="hidden" asp-route-Id="@Model.Id" />
        <input type="submit" value="Delete" class="btn btn-danger" /> |
        < <a asp-action="ListRole" asp-controller="Admin" class="btn btn-primary">Cancel</a>
    </form>
</div>


